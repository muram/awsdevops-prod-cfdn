{
  "StackId": {
    "Description": "Cloudformation Stack Id",
    "Value": { "Ref": "AWS::StackId" }
  },
  "StackName": {
    "Description": "Cloudformation Stack Name",
    "Value": { "Ref": "AWS::StackName" }
  },
  "VPCId": {
    "Description": "VPC Id",
    "Value": {
      "Ref": "VPC"
    },
    "Export": {
      "Name": {
        "Fn::Sub": "${AWS::StackName}-VPCId"
      }
    }
  },
  "VPCCidr": {
    "Description": "The CidrBlock of the VPC",
    "Value": { 
      "Fn::GetAtt": ["VPC", "CidrBlock"]
    },
    "Export": {
      "Name": {
        "Fn::Sub": "${AWS::StackName}-VPCCidr"
      }
    }
  },
  "PublicSubnets": {
    "Description": "List of Public Subnet Ids",
    "Value": {
      "Fn::If": [
        "TwoAZs",
        {
          "Fn::Join": [
            ",",
            [
              { "Ref": "PublicSubnetA" },
              { "Ref": "PublicSubnetB" }
            ]
          ]
        },
        {
          "Fn::If": [
            "ThreeAZs",
            {
              "Fn::Join": [
                ",",
                [
                  { "Ref": "PublicSubnetA" },
                  { "Ref": "PublicSubnetB" },
                  { "Ref": "PublicSubnetC" }
                ]
              ]
            },
           {
              "Fn::Join": [
                ",",
                [
                  { "Ref": "PublicSubnetA" },
                  { "Ref": "PublicSubnetB" },
                  { "Ref": "PublicSubnetC" },
                  { "Ref": "PublicSubnetD" },
                  { "Ref": "PublicSubnetE" },
                  { "Ref": "PublicSubnetF" }
                ]
              ]
            }
          ]
        }
      ]
    },
    "Export": {
      "Name": {
        "Fn::Sub": "${AWS::StackName}-PublicSubnets"
      }
    }
  },
  "PublicSubnetA": {
    "Description": "Public Subnet A's Id",
    "Value": { "Ref": "PublicSubnetA" },
